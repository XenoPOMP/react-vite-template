{
  "name": "react-vite-template",
  "private": true,
  "version": "1.7.5",
  "type": "module",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "dev:host": "yarn dev --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clear": "concurrently \"yarn clear:snaps\"",
    "clear:snaps": "shx rm -rf \"src/__tests__/__snapshots__\"",
    "test": "yarn clear && vitest",
    "test:frontend": "cross-env VITE_TESTING_MODE=FRONTEND yarn test",
    "test:backend": "cross-env VITE_TESTING_MODE=BACKEND yarn test",
    "test:fullstack": "cross-env VITE_TESTING_MODE=FULLSTACK yarn test",
    "coverage": "cross-env VITE_TESTING_MODE=FULLSTACK yarn clear && vitest run --coverage"
  },
  "dependencies": {
    "@pacote/react-use-outside": "^2.0.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "^20.4.1",
    "axios": "1.4.0",
    "axios-jwt": "^3.0.2",
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.18",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-document-meta": "3.0.0-beta.2",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-ios-emojis": "^1.0.2",
    "react-media": "^1.10.0",
    "react-query": "^3.39.3",
    "react-redux": "8.1.1",
    "react-router-dom": "^6.14.1",
    "react-scroll": "^1.8.9",
    "react-select": "^5.7.3",
    "react-swipeable": "^7.0.1",
    "redux": "4.2.1",
    "redux-deep-persist": "^1.0.7",
    "redux-persist": "^6.0.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "socket.io-client": "4.7.1",
    "tailwindcss-typescript": "^1.1.2",
    "that-sparks": "^0.1.8",
    "ts-custom-error": "^3.3.1",
    "universal-cookie": "^4.0.4",
    "use-sound": "^4.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-helmet": "^6.1.6",
    "@types/react-scroll": "^1.8.7",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.33.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.25",
    "prettier": "2.8.1",
    "prettier-init": "^1.3.0",
    "rvt-cli": "^1.2.3",
    "shx": "^0.3.4",
    "tailwindcss": "^3.3.2",
    "tsc": "^2.0.4",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
